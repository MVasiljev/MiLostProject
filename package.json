{
  "name": "milost",
  "version": "0.1.0-alpha.10",
  "description": "A Rust-like TypeScript library with WebAssembly optimizations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "*.wasm",
    "*.js"
  ],
  "scripts": {
    "build:standalone": "bash scripts/build-standalone.sh",
    "clean": "node scripts/build.js --clean",
    "build:wasm": "node scripts/build.js --wasm",
    "build:ts": "node scripts/build.js --ts",
    "build": "node scripts/build.js --all"
  },
  "keywords": [
    "rust",
    "typescript",
    "wasm",
    "webassembly"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "http-server": "^14.1.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wasm": {
      "import": "./dist/wasm-index.js",
      "require": "./dist/wasm-index.js",
      "types": "./dist/wasm-index.d.ts"
    },
    "./wasm/*": "./dist/wasm/*"
  }
}
