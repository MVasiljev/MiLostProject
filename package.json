{
  "name": "milost",
  "version": "0.1.0",
  "description": "A Rust-like TypeScript library with WebAssembly optimizations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "clean": "node scripts/build.js --clean",
    "build:wasm": "node scripts/build.js --wasm",
    "build:ts": "node scripts/build.js --ts",
    "build": "node scripts/build.js --all",
    "dev": "npx http-server -p 8080 --mime-types='{\"js\":\"application/javascript\",\"wasm\":\"application/wasm\"}' -c-1",
    "test": "echo \"No tests specified\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rust",
    "typescript",
    "wasm",
    "webassembly"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
